        "gulp-postcss": "9.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-squoosh": "^0.9.0",
        "gulp-svgmin": "4.1.0",
        "gulp-svgstore": "^9.0.0",
        "gulp-ttf2woff": "^1.1.1",
        "gulp-ttf2woff2": "^4.0.1",
        "gulp-zip": "5.1.0",
        "html-webpack-plugin": "5.5.0",
        "htmlhint": "1.1.2",
        "husky": "7.0.4",
        "image-webpack-loader": "^8.1.0",
        "jest": "27.4.7",
        "lint-staged": "12.3.3",
        "mini-css-extract-plugin": "2.5.3",
        "nunjucks": "3.2.3",
        "postcss": "8.4.6",
        "postcss-loader": "6.2.1",
        "postcss-preset-env": "7.3.0",
