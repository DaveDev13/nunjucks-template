{
    "private": true,
    "name": "nunjucks-template",
    "version": "1.0.0",
    "description": "Web starter kit",
    "engines": {
        "node": ">=16.13",
        "npm": ">=8.1"
    },
    "scripts": {
        "create": "node presets.js",
        "start": "cross-env NODE_ENV=development gulp default",
        "build": "cross-env NODE_ENV=production gulp build",
        "font": "cross-env NODE_ENV=production gulp font",
        "image:optimize": "cross-env NODE_ENV=production gulp image:optimize",
        "del:extra-files": "cross-env NODE_ENV=production gulp del:extra-files",
        "lint:scripts": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:styles": "gulp styles:lint",
        "lint:html": "gulp html:lint",
        "lint": "concurrently --kill-others \"eslint\" \"stylelint '**/*.(css|scss)'\" \"htmlhint'\"",
        "prepare": "husky install",
        "pre-commit": "lint-staged",
        "zip": "cross-env NODE_ENV=production gulp zip"
    },
    "repository": "git+https://github.com/DaveDev13/nunjucks-template.git",
    "keywords": [
        "gulp",
        "webpack",
        "davedev",
        "starter",
        "kit"
    ],
	"author": "Denis Kobelev | DaveDev <deniskobeleff@yandex.ru>",
    "license": "MIT",
    "homepage": "https://github.com/DaveDev13/nunjucks-template#readme",
    "devDependencies": {
        "@babel/core": "^7.17.0",
        "@babel/eslint-parser": "7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/register": "^7.17.0",
        "@ronilaukkarinen/gulp-stylelint": "14.0.5",
        "babel-loader": "^8.2.3",
        "browser-sync": "^2.27.7",
        "concurrently": "7.0.0",
        "connect-history-api-fallback": "1.6.0",
        "core-js": "^3.21.0",
        "cross-env": "^7.0.3",
        "css-loader": "6.6.0",
        "del": "^6.0.0",
        "eslint": "^8.8.0",
        "eslint-webpack-plugin": "^3.1.1",
        "file-loader": "^6.2.0",
        "glslify-loader": "^2.0.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-data": "^1.3.1",
        "gulp-fonter": "^0.3.0",
        "gulp-hash-filename": "^4.0.0",
        "gulp-htmlhint": "4.0.1",
        "gulp-htmlmin": "^5.0.1",
        "gulp-if": "^3.0.0",
        "gulp-imagemin": "7.1.0",
        "gulp-inject": "^5.0.5",
        "gulp-notify": "^4.0.0",
